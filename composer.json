{
  "name": "soundasleep/openclerk",
  "description": "An open source project to keep track of cryptocurrency finances",
  "autoload": {
    "psr-4": {
      "Migrations\\": "migrations",
      "DiscoveredComponents\\": "generated/components",
      "Core\\": "core"
    }
  },
  "require": {
    "soundasleep/html5lib-php": "~0.1.3",
    "lightopenid/lightopenid": "dev-master",
    "soundasleep/spritify": "dev-master",
    "kwipi/famfamfam-flags": "0.1.1",
    "openclerk/apis": "dev-master",
    "openclerk/commodities": "dev-master",
    "openclerk/country-list": "~1.0",
    "openclerk/currencies": "dev-master",
    "openclerk/cryptocurrencies": "dev-master",
    "openclerk/db": "dev-master",
    "openclerk/emails": "dev-master",
    "openclerk/events": "dev-master",
    "openclerk/exceptions": "dev-master",
    "openclerk/exchanges": "dev-master",
    "openclerk/fiat": "dev-master",
    "openclerk/jobs": "dev-master",
    "openclerk/i18n": "dev-master",
    "openclerk/config": "dev-master",
    "openclerk/metrics": "dev-master",
    "openclerk/routing": "dev-master",
    "openclerk/pages": "dev-master",
    "openclerk/templates": "dev-master",
    "soundasleep/component-discovery": "dev-master",
    "soundasleep/asset-discovery": "dev-master",
    "soundasleep/translation-discovery": "dev-master",
    "monolog/monolog": "~1.11",
    "mthaml/mthaml": "dev-master"
  },
  "require-dev": {
    "soundasleep/component-tests": "dev-master",
    "phpunit/phpunit": "4.0"
  },
  "repositories": [{
    "type": "vcs",
    "url": "https://github.com/soundasleep/lightopenid"
  }]
}
